@incollection{Trusler10.1039/9781782627043-00152,
    author = {Martin Trusler, J. P.},
    isbn = {978-1-84973-899-6},
    title = "{Virial Coefficients}",
    booktitle = "{Volume Properties: Liquids, Solutions and Vapours}",
    publisher = {The Royal Society of Chemistry},
    year = {2014},
    month = {12},
    abstract = "{The virial equation of state of a gas is a series expansion of the pressure in powers of inverse molar volume, the mth coefficient of which is given by a set of exact cluster integrals involving m molecules. For mixtures, the composition dependence of the virial coefficients can be expressed exactly in terms of interaction virial coefficients and the corresponding unlike cluster integrals. The series converges rapidly for gas-like states and expressions for all thermodynamic properties of the gas can be developed in terms of the virial coefficients. Numerous experimental techniques have been developed for the measurement of second and third virial coefficients and these may be categorised into direct and indirect methods. Among the direct techniques, the Burnet apparatus and the two-sinker magnetically-coupled densimeter are perhaps the most powerful. Excellent indirect determinations of virial coefficients have been made by means of both flow calorimetry and sound-speed measurements. Databases of critically-evaluated second and third virial coefficients of pure gases and mixtures are available. Finally, for less-well studied compounds, correlation methods based on the principle of corresponding states can be used to estimate pure-component and interaction virial coefficients.}",
    doi = {10.1039/9781782627043-00152},
    url = {https://doi.org/10.1039/9781782627043-00152},
}


