\def\eqHumidityRatioSnd#1{\ensuremath{#1 &= \frac{\symMolecularWeight{\nameV}{}}{\symMolecularWeight{\nameG}{}}   \frac{\symCompL{}_\nameV}{1-{\symCompL{}_\nameV}}}} % #1 nombre variable independiente, #2 M = peso molecular

\def\eqCompVapor#1{\ensuremath{\symCompL{}_\nameV = \frac{\HumidityRatio \frac{\symMolecularWeight{\nameV}{}}{\symMolecularWeight{\nameG}{}}}{1 + \HumidityRatio \frac{\symMolecularWeight{\nameV}{}}{\symMolecularWeight{\nameG}{}}}}}
