% from llama AI
\DeclareRobustCommand\sqrt{\@ifnextchar[{\@sqrt}{\@sqrt[0pt]}}
\def\@sqrt[#1]{\sqrt@{#1}}
\def\sqrt@#1#2{\root#1\of{#2}}

\def\vanDerWaals#1#2#3{\ensuremath{#1 = \frac{\univConstGasEos #3}{#2 - b} - \frac{a}{#2^2}}}
\def\eqDieterici#1#2#3{\ensuremath{#1 = \displaystyle{{\univConstGasEos #3} e^{-a/#2 \univConstGasEos  #3}  \over #2 - b} }}
\def\eqBerthelot#1#2#3{\ensuremath{#1 = \frac{\univConstGasEos #3}{#2 - b - \frac{a}{#3 #2^2}}}}

% arg #1: Volumen
\def\zVirial#1{\ensuremath{{1}+{\frac {\bCoeffVirial{}}{#1}}+{\frac {\cCoeffVirial{}}{#1^{2}}} + \cdots}} % la expresión z para la EOF virial
\def\eqVirialA#1#2#3{\ensuremath{#1 #2 = \univConstGasEos #3({B_1}+\frac{\bCoeffVirial{}}{#2} + \frac{\cCoeffVirial{}}{#2^2} + \frac{B_4}{#2^3} + \cdots)}}
\def\eqVirialB#1#2#3{\ensuremath{\frac {#1 #2}{\univConstGasEos #3} = {1}+{\frac {\bCoeffVirial{}}{#2}}+{\frac {\cCoeffVirial{}}{#2^{2}}} + \cdots}}
\def\eqVirialMean{}{}

\def\eqSndVirialCoeff#1#2#3{\ensuremath{{\bCoeffVirial{}}(#3)} = \frac{N_A}{2 \volume \omega^2} \iint {f_{#1#2}} d{\tau_#1 d{\tau_#2}}}
