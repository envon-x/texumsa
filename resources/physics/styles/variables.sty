% símbolo asignado para representar la masa
\def\symMass{\ensuremath{{m}}}

% Simbolo del peso molecular
\def\symMolecularWeight{\ensuremath{M}}

% nombre del 1er componente - gas
\def\nameG{\ensuremath{{\text{g}}}}

% nombre del 2do componente - vapor
\def\nameV{\ensuremath{{\text{v}}}} % Sin text{}, \ce{v} crea una flecha hacia abajo, cuando este no es la intención

% Representacion para la presión
\def\pressure{\ensuremath{\symPressure{}{}}}
\def\partialPressure{\ensuremath{\symPressure{i}{}}}
\def\totalPressure{\ensuremath{\symPressure{tot}{}}}

% presión de vapor
\def\Pvap{\ensuremath{P_{\ce{vap}}}}

% presión total
\def\Ptotal{\ensuremath{P_{\ce{tot}}}}

%presión de vapor saturado
\def\PvapSat{\ensuremath{P_{\ce{vap}}^{\degree}}}





% factor de compresibilidad
\def\zFactor{\ensuremath{z}}

% la relacion de humedad o humedad absoluta
\def\HumidityRatio{Y}

% Humedad relativa
\def\RelativeHumidity{\phi}
\def\DryBulbTemperature{\ensuremath{T_{\ce{db}}}} % Temperatura de bulbo seco (dry-bulb)
\def\WetBulbTemperature{\ensuremath{T_{\ce{wb}}}} % emperatura de bulbo húmedo (wet-bulb) \\
\def\DewPoint{\ensuremath{T_{\ce{dp}}}} % Punto de Rocío (dew point)


\def\Velocity{\ensuremath{u}}
\def\Density{\ensuremath{\rho}}
\def\Entropy{\ensuremath{S}}

% Grados de libertad 
\def\freeDegree{\ensuremath{F}}
\def\componentNumber{\ensuremath{C}}
\def\phaseNumber{\ensuremath{P}}


%Coeficientes viriales para la mezcla de componentes
\def\bCoeffVirialMixture{\ensuremath{B_{\text{mix}}}}
\def\cCoeffVirialMixture{\ensuremath{C_{\text{mix}}}}


\def\molarVolumeSaturate{\ensuremath{\hat{V}_{\text{Ve}}}}

%% Volumen molar para la mezcla
\def\symMolarVolumeForMixture{\ensuremath{{\bar V}_{\text{m}}}}

\def\isothermalCompresibility{\ensuremath{\kappa}}
\def\henryLaw{\ensuremath{k_H}}



