\newcommand{\symVaporMass}{\ensuremath{m_{\symVapor}}}                                      % Símbolo de la masa del vapor
% \newcommand{\eqnVaporMass}{\ensuremath{\symVaporMass &= \frac{a}{b}}}         % Fórmula de la masa del vapor
% \newcommand{\unitVaporMass}{\unit}                                                    % Unidad de la masa del vapor

\newcommand{\symGasMass}{\ensuremath{m_{\symGas}}}                                        % Símbolo del gas
% \newcommand{\eqnGasMass}{\ensuremath{\symGasMass &= \frac{a}{b}}}         % Fórmula del gas
% \newcommand{\uniGasMass}{\unit}                                                   % Unidad del gas

\newcommand{\symRelativeHumidity}{\ensuremath{\phi}}                                        % Símbolo de la Humedad relativa
\newcommand{\eqnRelativeHumidity}{\ensuremath{\symRelativeHumidity &= \frac{a}{b}}}         % Fórmula de la Humedad relativa
% \newcommand{\unitRelativeHumidity}{\unit}                                                 % Unidad de la Humedad relativa

\newcommand{\symAbsoluteHumidity}{\ensuremath{\rho_v}}                                      % Símbolo de la Humedad absoluta
\newcommand{\eqnAbsoluteHumidity}{\ensuremath{\symAbsoluteHumidity &= \frac{\symVaporMass{}}{\symGasMass}}}         % Fórmula de la Humedad absoluta
